		"require": "./dist/cia.js",
		"types": "./dist/cia.d.ts"
	},
	"./smdh": {
		"import": "./dist/smdh.js",
		"require": "./dist/smdh.js",
		"types": "./dist/smdh.d.ts"
	},
	"./ticket": {
		"import": "./dist/ticket.js",
		"require": "./dist/ticket.js",
		"types": "./dist/ticket.d.ts"
	},
	"./tmd": {
		"import": "./dist/tmd.js",
		"require": "./dist/tmd.js",
		"types": "./dist/tmd.d.ts"
	}
},
}
